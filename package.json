{
  "name": "JATE",
  "version": "1.0.0",
  "description": "A simple text editor",
  "main": "server/server.js",
  "scripts": {
    "install": "cd client && npm install && cd ../server && npm install --ignore-scripts",
    "start:dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server/server.js",
    "server": "nodemon server/server.js", 
    "client": "cd client && npm start"
  },
  "keywords": [],
  "author": "Raymond",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
