{
  "name": "JATE",
  "version": "1.0.0",
  "description": "A simple text editor","scripts": {
    "start:dev": "concurrently \"npm run server\" \"npm run dev --prefix client\"",
    "start": "node server/server.js",
    "server": "nodemon server.js --ignore client",
    "build": "webpack --mode production --config client/webpack.config.js",
    "client": "npm start --prefix client",
    "heroku-postbuild": "npm run build --prefix client"
  }
,  
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "main": "server/server.js"
}
